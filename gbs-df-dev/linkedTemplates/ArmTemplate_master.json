{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "gbs-df-dev"
		},
		"sqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldb'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=gbsintserver.database.windows.net;Initial Catalog=gbsintdev;User ID=procServiceDev"
		},
		"MyKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://gbs-key-valut-dev.vault.azure.net/"
		},
		"gbsstorage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "gbsintstoragekey"
		},
		"UserDetailsStorageTrigger_Dev_properties_user_details_parameters_Filename": {
			"type": "string",
			"defaultValue": "@triggerBody().fileName"
		},
		"UserDetailsStorageTrigger_Dev_properties_typeProperties_blobPathBeginsWith": {
			"type": "string",
			"defaultValue": "/suppliers/blobs/user_details/"
		},
		"UserDetailsStorageTrigger_Dev_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/10b43936-d253-4e38-85cc-909155bf077f/resourceGroups/AzureLearn/providers/Microsoft.Storage/storageAccounts/gbsintdev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "gbs-df-dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sqldb_connectionString": {
						"value": "[parameters('sqldb_connectionString')]"
					},
					"MyKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('MyKeyVault_properties_typeProperties_baseUrl')]"
					},
					"gbsstorage_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('gbsstorage_properties_typeProperties_connectionString_secretName')]"
					},
					"UserDetailsStorageTrigger_Dev_properties_user_details_parameters_Filename": {
						"value": "[parameters('UserDetailsStorageTrigger_Dev_properties_user_details_parameters_Filename')]"
					},
					"UserDetailsStorageTrigger_Dev_properties_typeProperties_blobPathBeginsWith": {
						"value": "[parameters('UserDetailsStorageTrigger_Dev_properties_typeProperties_blobPathBeginsWith')]"
					},
					"UserDetailsStorageTrigger_Dev_properties_typeProperties_scope": {
						"value": "[parameters('UserDetailsStorageTrigger_Dev_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}