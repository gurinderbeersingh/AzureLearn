{
	"name": "user_details",
	"properties": {
		"activities": [
			{
				"name": "Copy UserDetail",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "Truncate table spl.supplier",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ExcelUserDetail",
						"type": "DatasetReference",
						"parameters": {
							"Filename": {
								"value": "@pipeline().parameters.Filename",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "SQLSupplier",
						"type": "DatasetReference",
						"parameters": {
							"table": {
								"value": "@pipeline().parameters.table",
								"type": "Expression"
							},
							"schema": {
								"value": "@pipeline().parameters.schema",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"Filename": {
				"type": "string",
				"defaultValue": "user_details.xlsx"
			},
			"table": {
				"type": "string",
				"defaultValue": "supplier"
			},
			"schema": {
				"type": "string",
				"defaultValue": "spl"
			}
		},
		"annotations": []
	}
}